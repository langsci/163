% Time-stamp: <2010-12-13 17:25:56 asudeh>% Modified version of Mary Dalrymple's tree macros% Include file pst-tree.sty%Tree definitions to go with pstricks\RequirePackage{pstricks,pst-node}\include{pst-tree}%% \treespace moves a tree root over by the amount of the optional%% argument and offsets the right hand side correspondingly. This is%% useful if trees get crowded due to short words displaying above%% long words.\newcommand{\treespace}[2][2em]{\hspace*{#1}{#2}\hspace*{#1}}\def\tree{\@ifnextchar[{\@tree}{\@tree[top]}}\def\@tree[#1]#2#3{%  \renewcommand\psedge{\ncdiag[arm=0,angleA=270,angleB=90,linewidth=.5pt]}% original: \pstree[levelsep=*2.8ex,nodesep=3pt,treesep=.5cm]{\TR[name=#1]{#2}}{#3}}%  \pstree[levelsep=*2.8ex,nodesepA=0pt,nodesepB=3pt,treesep=.5cm]{\TR[name=#1]{#2}}{#3}}\def\talltree{\@ifnextchar[{\@talltree}{\@talltree[top]}}\def\@talltree[#1]#2#3{%  \renewcommand\psedge{\ncdiag[arm=0,angleA=270,angleB=90,linewidth=.5pt]}  \pstree[levelsep=*3.5ex,nodesep=3pt,treesep=.5cm]{\TR[name=#1]{#2}}{#3}}\def\talltalltree{\@ifnextchar[{\@talltalltree}{\@talltalltree[top]}}\def\@talltalltree[#1]#2#3{%  \renewcommand\psedge{\ncdiag[arm=0,angleA=270,angleB=90,linewidth=.5pt]}  \pstree[levelsep=*4.5ex,nodesep=3pt,treesep=.5cm]{\TR[name=#1]{#2}}{#3}}% Skeleton tree (no node labels)\def\skeletonroot{\@ifnextchar[{\@skeleton}{\@skeletonroot[top]}}\def\@skeletonroot[#1]#2#3{%  \renewcommand\psedge{\ncdiag[arm=0,angleA=270,angleB=90,linewidth=.5pt]} \pstree[levelsep=*2.8ex,nodesep=0pt,treesep=.5cm]{\TR[name=#1]{#2}}{#3}}\def\skeleton{\@ifnextchar[{\@skeleton}{\@skeleton[top]}}\def\@skeleton[#1]#2#3{%  \renewcommand\psedge{\ncdiag[arm=0,angleA=270,angleB=90,linewidth=.5pt]} \pstree[levelsep=*2.8ex,nodesepB=3pt,treesep=.5cm]{\TR[name=#1]{#2}}{#3}}\def\antree{\@ifnextchar[{\@tree}{\@tree[top]}}\def\@antree[#1]#2#3#4{%  \renewcommand\psedge{\ncdiag[arm=0,angleA=270,angleB=90,linewidth=.5pt]}  \pstree[levelsep=*2.8ex,nodesep=3pt,treesep=.5cm]{\TR[name=#1]{#2}}{\begin{tabular}[c]#4\\#3\end{tabular}}}\def\antalltree{\@ifnextchar[{\@talltree}{\@talltree[top]}}\def\@antalltree[#1]#2#3#4{%  \renewcommand\psedge{\ncdiag[arm=0,angleA=270,angleB=90,linewidth=.5pt]}  \pstree[levelsep=*3.5ex,nodesep=3pt,treesep=.5cm]{\TR[name=#1]{\begin{tabular}[c]#3\\#2\end{tabular}}}{#4}}\def\antalltalltree{\@ifnextchar[{\@talltalltree}{\@talltalltree[top]}}\def\@antalltalltree[#1]#2#3#4{%  \renewcommand\psedge{\ncdiag[arm=0,angleA=270,angleB=90,linewidth=.5pt]}  \pstree[levelsep=*4.5ex,nodesep=3pt,treesep=.5cm]{\TR[name=#1]{#2}}{\begin{tabular}[c]#4\\#3\end{tabular}}}\def\ctree{\@ifnextchar[{\@ctree}{\@ctree[top]}}\def\@ctree[#1]#2#3{%  \hbox{\begin{tabular}{@{}c@{}}{\@tree[{#1}]{#2}{#3}}\end{tabular}}}\def\ctalltree{\@ifnextchar[{\@ctalltree}{\@ctalltree[top]}}\def\@ctalltree[#1]#2#3{%  \hbox{\begin{tabular}{@{}c@{}}{\@talltree[{#1}]{#2}{#3}}\end{tabular}}}\def\nonodetree#1{%  \renewcommand\psedge{\ncdiag[arm=0,angleA=270,angleB=90,linewidth=.5pt]}  \hbox{\begin{tabular}{@{}c@{}}{%        \pstree[levelsep=*3.5ex,nodesepA=0pt,nodesepB=3pt,treesep=.5cm]{\Tp}{#1}}        \end{tabular}}}\def\le{\@ifnextchar[{\@le}{\@le[lex]}}\def\@le[#1]#2{\TR[name={#1}]{#2}}\def\tri#1{\setbox1\hbox{#1}%           \pstree[levelsep=2ex,nodesep=3pt,treesep=.5cm]{%                   \Tfan[fansize=\wd1,linewidth=.5pt]}{\box1}}% Right side to right side, with scalable arm.\def\rtor{\@ifnextchar[{\@rtor}{\@rtor[.15]}}\def\@rtor[#1]#2#3{%% original:  \ncangles[angle=0,armA={#1},linearc=.15,nodesep=-2pt,linewidth=.5pt]{#2}{#3}}  \ncangles[angle=0,armA={#1},linearc=.15,nodesep=-0.1em,linewidth=.5pt]{#2}{#3}}% Right side to right side, with scalable arm; curved.\def\rtorarc{\@ifnextchar[{\@rtorarc}{\@rtorarc[.15]}}\def\@rtorarc[#1]#2#3{%  \nccurve[ncurv={#1},nodesep=-0.1em,linewidth=.5pt]{#2}{#3}}% Right side to right side, with scalable arm, tight fit on first node\def\tightrtor{\@ifnextchar[{\@tightrtor}{\@tightrtor[.15]}}\def\@tightrtor[#1]#2#3{%  \ncangles[angle=0,armA={#1},linearc=.15,nodesepA=-3pt,nodesepB=-2pt,linewidth=.5pt]{#2}{#3}}% Left to left, directable entry points.\def\ltol{\@ifnextchar[{\@ltol}{\@ltol[0]}}\def\@ltol[#1]#2{\@ifnextchar[{\@@ntona[#1]{#2}}{\@@ntona[#1]{#2}[0]}}% Left to right, directable entry points.\def\ltor{\@ifnextchar[{\@ltor}{\@ltor[0]}}\def\@ltor[#1]#2{\@ifnextchar[{\@@ntona[#1]{#2}}{\@@ntona[#1]{#2}[180]}}% Right to left, directable entry points.\def\rtol{\@ifnextchar[{\@rtol}{\@rtol[180]}}\def\@rtol[#1]#2{\@ifnextchar[{\@@ntona[#1]{#2}}{\@@ntona[#1]{#2}[0]}}% Bottom of one node to bottom of another, scalable arm\def\btob{\@ifnextchar[{\@btob}{\@btob[.15]}}\def\@btob[#1]#2#3{%  \ncbar[arm={#1},angle=-90,nodesep=2pt,linewidth=.5pt]{#2}{#3}}% Top of one node to top of another, scalable arm\def\ttot{\@ifnextchar[{\@ttot}{\@ttot[.15]}}\def\@ttot[#1]#2#3{%  \ncbar[arm={#1},angle=90,nodesep=2pt,linewidth=.5pt]{#2}{#3}}% Node to node, with arrow.\def\ntona{\@ifnextchar[{\@ntona}{\@ntona[0]}}\def\@ntona[#1]#2{\@ifnextchar[{\@@ntona[#1]{#2}}{\@@ntona[#1]{#2}[0]}}\def\@@ntona[#1]#2[#3]#4{%   \nccurve[nodesepA=2pt,nodesepB=0pt,angleA={#1},angleB={#3},linewidth=.5pt]{->}{#2}{#4}}% Node to node, no arrow.\def\nton{\@ifnextchar[{\@nton}{\@nton[0]}}\def\@nton[#1]#2{\@ifnextchar[{\@@nton[#1]{#2}}{\@@nton[#1]{#2}[0]}}\def\@@nton[#1]#2[#3]#4{%   \nccurve[nodesepA=2pt,nodesepB=-1pt,angleA={#1},angleB={#3},linewidth=.5pt]{#2}{#4}}% Node to node, with arrow, tight fit at beginning of line.\def\tightntona{\@ifnextchar[{\@tightntona}{\@tightntona[0]}}\def\@tightntona[#1]#2{\@ifnextchar[{\@@tightntona[#1]{#2}}{\@@tightntona[#1]{#2}[0]}}\def\@@tightntona[#1]#2[#3]#4{%   \nccurve[nodesepA=-2pt,nodesepB=0pt,angleA={#1},angleB={#3},linewidth=.5pt]{->}{#2}{#4}}% Node to node dotted line, with arrow, tight fit at beginning of line.\def\tightntonad{\@ifnextchar[{\@tightntonad}{\@tightntonad[0]}}\def\@tightntonad[#1]#2{\@ifnextchar[{\@@tightntonad[#1]{#2}}{\@@tightntonad[#1]{#2}[0]}}\def\@@tightntonad[#1]#2[#3]#4{%   \nccurve[linestyle=dashed,nodesepA=-2pt,nodesepB=0pt,angleA={#1},angleB={#3},linewidth=.5pt]{->}{#2}{#4}}% Node to node dotted line, with arrow\def\ntonad{\@ifnextchar[{\@ntonad}{\@ntonad[0]}}\def\@ntonad[#1]#2{\@ifnextchar[{\@@ntonad[#1]{#2}}{\@@ntonad[#1]{#2}[0]}}\def\@@ntonad[#1]#2[#3]#4{%    \nccurve[linestyle=dashed,nodesepA=-1pt,nodesepB=0pt,angleA={#1},angleB={#3},linewidth=.5pt]{->}{#2}{#4}}% Node to node, no arrow, tight fit at beginning of line.\def\tightnton{\@ifnextchar[{\@tightnton}{\@tightnton[0]}}\def\@tightnton[#1]#2{\@ifnextchar[{\@@tightnton[#1]{#2}}{\@@tightnton[#1]{#2}[0]}}\def\@@tightnton[#1]#2[#3]#4{%   \nccurve[nodesepA=-2pt,nodesepB=-1pt,angleA={#1},angleB={#3},linewidth=.5pt]{#2}{#4}}% Node to node, two arrows, tight fit at beginning of line.\def\ntonaa{\@ifnextchar[{\@ntonaa}{\@ntonaa[0]}}\def\@ntonaa[#1]#2{\@ifnextchar[{\@@ntonaa[#1]{#2}}{\@@ntonaa[#1]{#2}[0]}}\def\@@ntonaa[#1]#2[#3]#4{%   \nccurve[nodesepA=2pt,nodesepB=0pt,angleA={#1},angleB={#3},linewidth=.5pt]{<->}{#2}{#4}}\def\ltord{\@ifnextchar[{\@ltord}{\@ltord[0]}}\def\@ltord[#1]#2{\@ifnextchar[{\@@ltord[#1]{#2}}{\@@ltord[#1]{#2}[180]}}\def\@@ltord[#1]#2[#3]#4{%   \nccurve[linestyle=dashed,nodesepA=2pt,nodesepB=1pt,angleA={#1},angleB={#3},linewidth=.5pt]{->}{#2}{#4}}