\newdimen\catdimen
\def \gloss #1#2{\leavevmode
        \vtop {\hbox {\strut \textit{#1}}
               \hbox {\deepstrut #2}}}
\def \cgloss #1#2{\mbox{\def\arraystretch{1}%
        \begin{tabular}[t]{@{}c@{}}{\strut \textit{#1}}\\{\deepstrut #2}\end{tabular}}}
\def \deepstrut {\hbox {\vrule height 1.5ex depth 1.2ex width 0pt}}
\def \lexentry #1#2{\setbox1\hbox{\textit{#1}}%
                      \ifdim \wd1 >3em \lexentrydimen=\wd1 %
                      \else \lexentrydimen=3em \fi%
                      \noindent%
                      \makebox[\lexentrydimen][l]{\box1}\hspace*{1em}\nobreak%
                      \feqs{#2}}
\def \catlexentry #1#2#3{\setbox1\hbox{\textit{#1}}%
                      \ifdim \wd1 >3em \lexentrydimen=\wd1 %
                      \else \lexentrydimen=3em \fi%
                      \noindent%
                      \makebox[\lexentrydimen][l]{\box1}\hspace*{1em}\nobreak%
                      \setbox2\hbox{\small #2}%
                      \ifdim \wd2 >1em \catdimen=\wd2 %
                      \else \catdimen=1em \fi%
                      \noindent%
                      \makebox[\catdimen][l]{\box2}\hspace*{1em}\nobreak%
                      \feqs{#3}}

% \feqs {<alignment material>}
\def \feqs #1{\begin{tabular}[t]{@{\strut}l@{\strut}}#1\end{tabular}}

\def \phraserule #1#2{\setbox1\hbox{#1}%
                      \ifdim \wd1 >2em \mothernodedimen=\wd1 %
                      \else \mothernodedimen=2em \fi%
   \leavevmode\hbox{\makebox[\mothernodedimen][l]{\box1}{$\rightarrow$}\hspace{.6em}#2}}

\def \rulenode #1{\begin{tabular}[t]{c}#1\end{tabular}}

\def \optrulenode#1{
  \setbox1\hbox{$\left(\hbox{\begin{tabular}{@{\strut}c@{\strut}}#1\end{tabular}}\right)$}
  \raisebox{1.9ex}{\raisebox{-\ht1}{\copy1}}}

\def \treenode #1{\renewcommand{\arraystretch}{1}
  \begin{tabular}{@{\strut}c@{\strut}}#1\end{tabular}}

\def \opttreenode#1{
  $\left(\hbox{\renewcommand{\arraystretch}{1}\begin{tabular}{@{\strut}c@{\strut}}#1\end{tabular}}\right)$}

